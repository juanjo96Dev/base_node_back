#
# APPLICATION
#
APP_NAME=BackNet
APP_SCHEMA=http
APP_HOST=localhost
APP_PORT=3000
APP_ROUTE_PREFIX=/api
APP_BANNER=true
APP_SECRETKEY=

#
# LOGGING
#
LOG_LEVEL=debug
LOG_OUTPUT=dev

#
# MORGAN
#
MORGAN_ENABLED=false

#
# TYPEORN
#
TYPEORM_CONNECTION=postgres
TYPEORM_URL=
TYPEORM_HOST='*****eu-west-1.compute.amazonaws.com'
TYPEORM_SYNCHRONIZE=false
TYPEORM_LOGGING=false
TYPEORM_LOGGER=advanced-console

#
# PATH STRUCTRUE
#
TYPEORM_MIGRATIONS=src/database/migrations/**/*.ts
TYPEORM_MIGRATIONS_DIR=src/database/migrations
TYPEORM_ENTITIES=src/api/models/**/*.ts
TYPEORM_ENTITIES_DIR=src/api/models
CONTROLLERS=src/api/controllers/**/*Controller.ts
MIDDLEWARES=src/api/middlewares/**/*Middleware.ts
SUBSCRIBERS=src/api/subscribers/**/*Subscriber.ts

#
# Metrics
#
METRICS_ENABLED=true

#
# Swagger
#
SWAGGER_ENABLED=true
SWAGGER_SCRAPER_ENABLED=true
SWAGGER_ROUTE=/swagger
SWAGGER_FILE=api/swagger.json
SWAGGER_USERNAME=admin
SWAGGER_PASSWORD=1234
